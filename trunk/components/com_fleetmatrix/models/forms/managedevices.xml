<?xml version="1.0" encoding="UTF-8"?>
<form name="subscription">
	<fieldset name="subscription">
        <field name="serialnumbers"
                type="textarea"
                label="Enter Range of SNs"
                description="Serial Numbers of Safety Controllers to add"
        	    required="true"
                rows="3"
                cols="50"></field>

        <field name="selectcompany"
               type="selectcompany"
               label="Assign Company"
               required="true"
               ></field>

        <field name="selectgroup"
               type="selectgroup"
               label="Assign Group"
               required="true"
               ></field>
        <field name="selectsubscription"
               type="selectsubscription"
               label="Exist Subscriptions"
               required="true"
               ></field>

	</fieldset>
</form>